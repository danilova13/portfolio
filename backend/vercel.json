{
	"version": 2,
	"builds": [
	  {
		"src": "index.js", 
		"use": "@vercel/node"
	  },
	  {
		"src": "*.jpg", 
		"use": "@vercel/static"
	  },
	  {
		"src": "*.jpeg", 
		"use": "@vercel/static"
	  },
	  {
		"src": "*.png", 
		"use": "@vercel/static"
	  },
	  {
		"src": "*.webp", 
		"use": "@vercel/static"
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "index.js"
	  }
	]
  }